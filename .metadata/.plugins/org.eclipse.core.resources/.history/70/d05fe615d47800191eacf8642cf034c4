package firstApplication;

public class Dick_test {

	public static double maxNum(int num1, int num2) {
		if (num1>num2) {
			return num1;
		} else {
			return (num2);
		}
		
	}
	public static double maxNum(int value1, int value2) {
		if (value1>value2) {
			return value1;
		} else {
			return (value2);
		}
	}
	public static void main (String[] args) {
		System.out.println(maxNum(1.2));
	}

}
